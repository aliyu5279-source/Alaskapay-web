{
  "netlify": {
    "siteId": "your-netlify-site-id",
    "token": "your-netlify-personal-access-token",
    "siteName": "your-site-name"
  },
  "vercel": {
    "projectId": "your-vercel-project-id",
    "token": "your-vercel-token",
    "teamId": "your-team-id-optional"
  },
  "encryption": {
    "enabled": true,
    "algorithm": "aes-256-gcm"
  },
  "versionControl": {
    "enabled": true,
    "maxVersions": 50,
    "autoBackup": true
  },
  "sync": {
    "defaultPlatforms": ["netlify", "vercel", "local"],
    "defaultEnvironment": "production",
    "excludeKeys": [
      "LOCAL_ONLY_VAR"
    ]
  }
}
